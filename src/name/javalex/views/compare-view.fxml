<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.BorderPane?>

<VBox prefHeight="700.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="name.javalex.controllers.CompareController">
    <ButtonBar buttonOrder="" prefHeight="42.0" prefWidth="600.0">
        <buttons>
            <Button fx:id="btnClose" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Back to main window" onAction="#closeCompare"/>
        </buttons>
        <VBox.margin>
            <Insets right="6.0" top="6.0" />
        </VBox.margin>
    </ButtonBar>

    <HBox prefHeight="671.0" prefWidth="1000.0">
        <children>
            <VBox>
                <Label prefHeight="17.0" prefWidth="280.0" text="Processes from opened XML (Groupped)" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets bottom="2.0" left="6.0" right="6.0" top="6.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Calibri Bold" size="14.0" />
                    </font>
                </Label>
                <TableView fx:id="openedTasks" prefHeight="643.0" prefWidth="295.0">
                    <columns>
                        <TableColumn fx:id="openedNameColumn" editable="false" prefWidth="195.0" resizable="false" text="Name" />
                        <TableColumn fx:id="openedMemoryColumn" editable="false" prefWidth="100.0" resizable="false" text="Memory (KB)" />
                    </columns>
                </TableView>
            </VBox>
            <VBox>
                <Label prefHeight="17.0" prefWidth="280.0" text="Current Processes (Groupped)" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets bottom="2.0" left="6.0" right="6.0" top="6.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Calibri Bold" size="14.0" />
                    </font>
                </Label>
                <TableView fx:id="currentTasks" prefHeight="654.0" prefWidth="305.0">
                    <columns>
                        <TableColumn fx:id="currentNameColumn" editable="false" prefWidth="194.0" resizable="false" text="Name" />
                        <TableColumn fx:id="currentMemoryColumn" editable="false" prefWidth="93.0" resizable="false" text="Memory (KB)" />
                    </columns>
                </TableView>
            </VBox>
        </children>
    </HBox>
</VBox>
